{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "react","prettier"
  ],
  "rules": {
    "semi": ["error", "always"],
    "quote-props": ["error", "as-needed", { "keywords": true }],
    "quotes": ["error", "single"],
    "no-mixed-requires": ["error"],
    "indent": "off",
    "camelcase": ["error", { "properties": "never" }],
    "prettier/prettier": "error",
    "react/jsx-curly-spacing": ["error", "never"]
  }
}
