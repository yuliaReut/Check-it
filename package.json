{
  "name": "check-it",
  "homepage": "https://yuliareut.github.io/Check-it/" ,
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "description": "Личный проект от Юлии Реутовой",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "eslint": "eslint --ext .jsx --ext .js src/ ",
    "test": "npm run eslint && npm run test.jest && npm run format:check",
    "test.jest": "jest",
    "format:check": "prettier --ckeck .",
    "lint": "npm run eslint && npm run format:check",

    "predeploy": "npm run build && replace-in-file ./build/index.html 'css/main.min.css' '/Check-it/css/main.min.css'",
    "deploy": "gh-pages -d build"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/src/style-mock.js",
      "^Project(.*)$": "<rootDir>/src$1"
    },
    "setupFilesAfterEnv": [
      "./setup-tests.js"
    ]
  },

  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@reduxjs/toolkit": "1.9.3",
    "@vitejs/plugin-react": "^4.2.1",
    "vite": "^5.1.6",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/react-hooks": "^5.1.0",
    "axios": "^0.21.1",
    "axios-mock-adapter": "^1.19.0",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.0.2",
    "eslint": "^8.0.0",
    "eslint-plugin-react": "^7.22.0",
    "file-loader": "^6.2.0",
    "history": "^4.10.1",
    "jest": "^26.6.3",
    "leaflet": "^1.7.1",
    "prettier": "^3.2.5",
    "redux-mock-store": "^1.5.4",
    "style-loader": "^2.0.0",
    "uuid": "^8.3.2",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.2",
    "eslint-config-prettier":"",
    "eslint-plugin-prettier":"",
    "typescript-eslint": "^7.7.1",
    "gh-pages": "^6.1.1",
    "replace-in-file":""
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.22.3",
    "redux": "^5.0.1",
    "reselect": "^4.0.0",
    "typescript": "^4.9.5",
    "dotenv": "^16.4.5",
    "query-string": "^7.1.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5"
  }
}
